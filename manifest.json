{
  "manifest_version": 3,
  "name": "Sudoku Hotkey",
  "version": "1.0",

  "description": "Allows you to turn on Note Mode from hitting any letter key",

  "icons": {
    "48": "icons/android-icon-48x48.png",
    "96": "icons/android-icon-96x96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://sudoku.com/*"],
      "js": ["sudoku-hotkey.js"]
    }
  ]
}
